// "'JetBrainsMono Nerd Font Mono'",
{
    // ./.vscode/settings.json
    //
    // â”€â”€â”€ âš™ï¸ GENERAL SETTINGS â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    "json.schemaDownload.enable": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.trimTrailingWhitespaceInRegexAndStrings": false,
    "prettier.useEditorConfig": true,
    "files.associations": {
        "*.json": "jsonc",
        ".editorconfig": "editorconfig"
    },
    //
    // â”€â”€â”€ ğŸ¨ WORKBENCH & APPEARANCE â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    "workbench.colorTheme": "One Dark Modern Pro",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "material-product-icons",
    //
    // â”€â”€â”€ ğŸ–¥ï¸ TERMINAL SETTINGS â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    "terminal.integrated.fontFamily": "'Hack Nerd Font Mono'",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.15,
    //
    // â”€â”€â”€ ğŸ“ EDITOR CORE SETTINGS â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    "editor.fontFamily": "'JetBrainsMono Nerd Font Mono'",
    "editor.fontSize": 14,
    "editor.lineHeight": 1.4,
    "editor.fontLigatures": true,
    "editor.smoothScrolling": true,
    "editor.minimap.enabled": false,
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.tabCompletion": "on",
    "editor.showDeprecated": true,
    "editor.autoIndent": "full",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.rulers": [
        120
    ],
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.defaultModel": "indentationModel",
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.guides.highlightActiveBracketPair": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.indentation": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    //
    // â”€â”€â”€ ğŸ“ CODE FOLDING â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    "editor.defaultFoldingRangeProvider": "EditorConfig.EditorConfig",
    "editor.foldingStrategy": "indentation",
    "editor.detectIndentation": false,
    //
    // â”€â”€â”€ ğŸ’» LANGUAGE SPECIFIC SETTINGS â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    //------ TypeScript ------//
    "betterTypeScriptErrors.prettify": true,
    "typescript.experimental.useTsgo": true,
    "typescript.format.enable": true,
    "typescript.format.semicolons": "insert",
    "typescript.preferences.quoteStyle": "single",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.suggest.paths": false,
    "typescript.tsserver.log": "off",
    "typescript.tsserver.enableTracing": false,
    "typescript.tsserver.useSyntaxServer": "always",
    "[typescript]": {
        "files.autoSave": "onFocusChange",
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.foldingStrategy": "auto",
        "editor.wordWrap": "on"
    },
    //------ JavaScript ------//
    "javascript.format.enable": true,
    "javascript.format.semicolons": "insert",
    "javascript.preferences.quoteStyle": "single",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggest.paths": false,
    "[javascript]": {
        "files.autoSave": "onFocusChange",
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.foldingStrategy": "auto",
        "editor.wordWrap": "on"
    },
    //------ Markdown ------//
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    //------ JSON / JSONC ------//
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    //
    // â”€â”€â”€ â• EXTENSION-SPECIFIC SETTINGS â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    //------ Better Comments ------//
    "better-comments.multilineComments": true,
    "better-comments.highlightPlainText": false,
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "fixme",
            "color": "#008000",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "fixit",
            "color": "#008000",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "fix",
            "color": "#008000",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "bug",
            "color": "#008000",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        }
    ],
    //------ ESLint ------//
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "eslint.format.enable": true,
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "eslint.codeAction.disableRuleComment": {
        "enable": true,
        "location": "sameLine"
    },
    "eslint.useFlatConfig": true,
    "eslint.options": {
        "overrideConfigFile": "eslint.config.ts"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    //------ EmogiSense ------//
    "emojisense.languages": {
        "markdown": true,
        "plaintext": false,
        "json": true,
        "scminput": true
    },
    //------ Indent Rainbow ------//
    "indentRainbow.indicatorStyle": "classic",
    "indentRainbow.lightIndicatorStyleLineWidth": 1,
    "indentRainbow.colorOnWhiteSpaceOnly": true,
    "indentRainbow.updateDelay": 100,
    "indentRainbow.ignoreErrorLanguages": [
        "haskell"
    ],
    "indentRainbow.errorColor": "rgba(128,32,32,0.6)",
    "indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
    "indentRainbow.colors": [
        "rgba(255,255,64,0.07)",
        "rgba(127,255,127,0.07)",
        "rgba(255,127,255,0.07)",
        "rgba(79,236,236,0.07)"
    ],
    //------ Bun ------//
    "bun.runtime": "%USERPROFILE%/.bun/bin",
    "bun.debugTerminal.enabled": true,
    "bun.debugTerminal.stopOnEntry": false,
    "bun.test.filePattern": "**/*{.test.,.spec.,_test_,_spec_}{js,ts,tsx,jsx,mts,cts,cjs,mjs}",
    "bun.test.customScript": "bun test",
    //
    // â”€â”€â”€ GIT SETTINGS â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    //
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.openRepositoryInParentFolders": "always",
    "[editorconfig]": {
        "editor.defaultFormatter": "EditorConfig.EditorConfig"
    },
    "typescript.tsdk": "node_modules/typescript/lib"
}